<tr id="item_{{id}}">
    <td><span class="content" /></td>
    <td>
      <span class="actions btn-group badge pull-right">
        {{#each actions}}
        <a data-action="{{css_class}}"
           class="{{css_class}} btn btn-default btn-xs"
           href="#">
            <span class="glyphicon {{icon}}" data-action="{{css_class}}"></span> {{title}}</a>
        {{/each}}
      </span>
    </td>
</tr>
